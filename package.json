{
  "name": "jq-tutorial",
  "version": "0.0.1",
  "description": "Exercises for learning jq",
  "scripts": {
    "test": "ava",
    "prepare": "tsc",
    "prestart": "tsc",
    "start": "node bin/jq-tutorial.js",
    "erase-progress": "rm src/_progress.txt"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 75,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "files": [
    "./dist",
    "./bin",
    "./data",
    "./problems"
  ],
  "bin": {
    "jq-tutorial": "./bin/jq-tutorial.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.11.2",
    "jshint": "^2.10.2",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "@ava/typescript": "^1.1.1",
    "ava": "^3.13.0",
  },
  "dependencies": {
    "async": "~0.2.9",
    "colors": "~0.6.2",
    "lodash": "^4.17.15"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nawhi/jq-tutorial.git"
  },
  "keywords": [
    "jq",
    "json",
    "tutorial",
    "exercises"
  ],
  "ava": {
    "verbose": true,
    "files": [
      "test/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "failWithoutAssertions": true,
    "require": [
      "ts-node/register"
    ]
  },
  "bugs": {
    "url": "https://github.com/nawhi/jq-tutorial/issues"
  },
  "author": "Nick White <nick@nickwhite.cc> (https://nickwhite.cc)",
  "license": "MIT"
}
