{
  "name": "jq-tutorial",
  "version": "0.0.5",
  "description": "Exercises for learning jq",
  "scripts": {
    "pretest": "tsc",
    "test": "mocha -r ts-node/register __tests__/**/*.test.ts",
    "prepack": "tsc",
    "start": "ts-node ./src",
    "erase-progress": "rm src/_progress.txt"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 60,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "bin": {
    "jq-tutorial": "./bin/jq-tutorial.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.162",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "jshint": "^2.10.2",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "@types/node": "^14.11.2",
    "typescript": "^4.0.3",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "async": "~0.2.9",
    "colors": "~0.6.2",
    "lodash": "^4.17.15"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nawhi/jq-tutorial.git"
  },
  "keywords": [
    "jq",
    "json",
    "tutorial",
    "exercises"
  ],
  "bugs": {
    "url": "https://github.com/nawhi/jq-tutorial/issues"
  },
  "private": true,
  "author": "Nick White <nick@nickwhite.cc> (https://nickwhite.cc)",
  "license": "MIT"
}
